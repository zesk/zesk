{
  "name": "zesk/zesk",
  "description": "The Zesk Application Framework",
  "require": {
    "php": ">=8.1",
    "psr/log": "^1.0",
    "components/jquery": "^3.2",
    "psr/cache": "^1.0",
    "ext-calendar": "*",
    "ext-json": "*",
    "ext-posix": "*"
  },
  "suggest": {
    "ext-xdebug": "*",
    "ext-curl": "*",
    "ext-imap": "*",
    "ext-readline": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "*",
    "phpunit/php-invoker": "*",
    "friendsofphp/php-cs-fixer": "^3.4"
  },
  "authors": [
    {
      "name": "Kent Davidson",
      "email": "getsupport@marketacumen.com"
    }
  ],
  "scripts": {
  },
  "keywords": [
    "zesk",
    "application",
    "framework"
  ],
  "autoload": {
    "psr-0": {
      "zesk\\": "./"
    }
  },
  "bin": [
    "bin/zesk",
    "bin/zesk-command.php",
    "bin/link-vendor-to-dev-zesk.sh",
    "bin/daemon-bouncer.sh",
    "bin/php-find.sh"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://zesk.com",
  "autoload-dev": {
    "classmap": [
      "modules/Database/test/zesk/DatabaseUnitTest.php",
      "modules/ORM/test/classes/ORMUnitTest.php",
      "modules/PHPUnit/classes/PHPUnit/TestCase.php",
      "modules/Repository/classes/TestCase.php"
    ]
  }
}
