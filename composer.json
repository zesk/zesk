{
  "name" : "zesk/zesk",
  "description" : "The Zesk Application Framework",
  "require" : {
    "php" : ">=5.4.0",
    "psr/log": "^1.0"
  },
  "require-dev" : {
    "phpunit/phpunit" : "^5.6",
    "phpunit/php-invoker" : "^1.1"
  },
  "authors" : [ {
    "name" : "Kent Davidson"
  } ],
