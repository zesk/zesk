{
  "name": "zesk/zesk",
  "description": "The Zesk Application Framework",
  "require": {
    "php": ">=8.1",
    "psr/log": ">=3",
    "components/jquery": "^3.2",
    "psr/cache": ">=3",
    "ext-calendar": "*",
    "ext-json": "*",
    "ext-posix": "*",
    "ext-pcntl": "*",
    "ext-intl": "*",
    "doctrine/orm": "*"
  },
  "suggest": {
    "ext-xdebug": "*",
    "ext-curl": "*",
    "ext-imap": "*",
    "ext-readline": "*"
  },
  "require-dev": {
    "phpunit/phpunit": ">=10",
    "phpunit/php-invoker": "*",
    "friendsofphp/php-cs-fixer": "*"
  },
  "authors": [
    {
      "name": "Kent Davidson",
      "email": "getsupport@marketacumen.com"
    }
  ],
  "scripts": {
  },
  "keywords": [
    "zesk",
    "application",
    "framework"
  ],
  "autoload": {
    "psr-0": {
      "zesk\\": "./src"
    }
  },
  "bin": [
    "bin/zesk",
    "bin/zesk-command.php",
    "bin/link-vendor-to-dev-zesk.sh",
    "bin/file-trigger.sh",
    "bin/php-find.sh"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://zesk.com",
  "autoload-dev": {
    "classmap": [
      "modules/PHPUnit/classes/PHPUnit/TestCase.php",
      "modules/Repository/zesk/Repository/TestCase.php"
    ]
  },
  "copyright": "Copyright &copy; 2023, Market Acumen, Inc."
}
