{
	"name": "Apache Integration",
	"description": "Provides services to integrate directly with the Apache web server.",
	"url_project": "https://httpd.apache.org/",
	"autoload": {
		"classPrefix": "zesk\\Apache\\"
	},
	"moduleClass": "zesk\\Apache\\Module",
	"environment": {
		"APACHECTL": {
			"type": "bin",
			"references": ["sbin/apache-bounce.sh"]
		},
		"APACHECTL_RESTART": {
			"type": "argument",
			"references": ["sbin/apache-bounce.sh"],
			"validation": {
				"type": "enum",
				"values": ["graceful",""]
			}
		}
	}
}
